<% require 'nokogiri' %>
<% require 'open-uri' %>
<% require 'matrix' %>
<%# load welcome_helper %>

<% re = /([A-z\s]+): ([A-Z0-9][A-Z\s\d,.\-|ot]+)/%>



<%nameArray = Array.new()%>


<%nameArray=add_name(nameArray,["CRATTY","WES"])%>



<% nameArray.size.times do |j| %>
	<% a=nameArray[0]%>
	<%url="http://svc.mt.gov/msl/mtcadastral/Search/SearchByOwner?ownerName=#{a[0]}%20#{a[1]}&countyId=13"%>

	<%data = Nokogiri::HTML(open(url))%>
	<% dataStr = data.to_s%>
	<%= a[1] %>
	<%= a[0] %>
	<br />
	<%result = dataStr.scan(re)%>
		<%result.size.times do |i|%>
			<%= result[i].to_s.gsub('"', '').gsub('[', '').gsub(']', '') %>
			<br />
		<% end %>
	<br />
<% end %>

<% %>